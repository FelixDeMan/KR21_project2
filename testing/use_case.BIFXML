    <!-- PCTs -->
   <DEFINITION>
        <FOR>cognitive-impairment</FOR>
        <TABLE>0.96 0.04</TABLE>
    </DEFINITION>
   <DEFINITION>
        <FOR>age-over-60</FOR>
